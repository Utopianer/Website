  "cleanUrls": true,
  "headers": [
    {
      "source": "/fonts/Inter.ttf",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/chat",
